{
  "name": "vision-wheel-dealers",
  "version": "0.1.0",
  "description": "Transform the existing Vision Wheel site and make it eCommerce enabled for the companyï¿½s retail dealers and OEM/OES customers",
  "main": "./server/server.js",
  "author": "Mirum",
  "license": "ISC",
  "config": {
    "port": "8080",
    "path": "./:./../common/"
  },
  "scripts": {
    "autoprefixer": "postcss -u --local-plugins -u autoprefixer -m file -r ./release/css/visionwheel.css",
    "env": "env",
    "inspector": "node-inspector",
    "build": "webpack --colors -p --progress",
    "build:watch": "webpack -v && webpack --colors --progress --watch",
    "server": "node $npm_package_main",
    "server-debug": "node --debug $npm_package_main",
    "server-debug-brk": "node --debug-brk $npm_package_main",
    "server:watch": "NODE_ENV=development DEBUG=vwd:* NODE_PATH=$npm_package_config_path nodemon --stack-trace-limit=1000 ./server/server.js --ignore ./release --ignore ./server/scripts",
    "server-debug:watch": "nodemon --debug $npm_package_main",
    "start": "NODE_PATH=$npm_package_config_path NODE_OPTIONS=--max-old-space-size=8192 npm run server",
    "start-debug": "NODE_PATH=$npm_package_config_path npm run server-debug",
    "start-debug-brk": "NODE_PATH=$npm_package_config_path npm run server-debug-brk",
    "start-qa": "NODE_ENV=qa DEBUG=vwd:*, npm start -- --no-color",
    "start-qa-docker": "docker build -t $npm_package_name . && docker run -it -e NODE_ENV=qa -p 8080:8080 $npm_package_name",
    "start-dev": "NODE_ENV=development Q_DEBUG=1 DEBUG=vwd:*, npm start",
    "start-dev-debug": "NODE_ENV=development Q_DEBUG=1 DEBUG=vwd:*, npm run start-debug",
    "start-dev-debug-brk": "NODE_ENV=development Q_DEBUG=1 DEBUG=vwd:*, npm run start-debug-brk",
    "start-stg": "NODE_ENV=staging Q_DEBUG= DEBUG=vwd:* npm start -- --no-color",
    "start-prod": "NODE_ENV=production Q_DEBUG= DEBUG=vwd:* npm start -- --no-color",
    "start-prod-debug": "NODE_ENV=production Q_DEBUG= DEBUG=vwd:*, npm run start-debug -- --no-color",
    "start-prod-debug-brk": "NODE_ENV=production Q_DEBUG= DEBUG=vwd:*, npm run start-debug-brk -- --no-color",
    "start-prod-docker": "docker build -t $npm_package_name . && docker run -it -e NODE_ENV=production -p 8080:8080 $npm_package_name",
    "docker-developer-mode": "npm run start & npm run build:watch",
    "docker-dev": "docker build -f Dockerfile.dev -t vwdev . && docker run  -p 8080:8080 -v `pwd`:/usr/src/app/site -w /usr/src/app/site -v `pwd/../common`:/usr/src/app/common -w /usr/src/app/common vwdev",
    "x----x----x": "x----x----x ALL `npm run test` COMMANDS x----x----x",
    "test": "mocha tests/models.js",
    "lint": "eslint -f table server",
    "lint-log": "eslint -f table ./server > eslint-debug.log"
  },
  "repository": {
    "type": "git",
    "url": "git@int-gitlab.aws.lbox.com:Mirum/vision-wheel-dealers.git"
  },
  "keywords": [
    "vision",
    "wheel",
    "dealers"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.0",
    "camelcase": "^3.0.0",
    "commander": "^2.9.0",
    "compression": "^1.6.2",
    "connect-pg-simple": "^3.1.2",
    "css-loader": "^0.23.1",
    "es5-shim": "^4.5.8",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "eslint-release": "^0.10.1",
    "express": "^4.13.4",
    "express-oauth-server": "latest",
    "express-session": "^1.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fuse.js": "^5.0.10-beta",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.3",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.1.2",
    "json-loader": "^0.5.4",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.11.2",
    "method-override": "^2.3.5",
    "moment": "^2.17.1",
    "node-fetch": "^1.6.3",
    "node-sass": "^3.7.0",
    "nodemon": "^1.11.0",
    "npm": "^3.8.9",
    "npm-license": "^0.3.2",
    "oauth2-server": "latest",
    "pug": "^2.0.0-beta6",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "redbox-react": "^1.3.1",
    "requirejs": "^2.2.0",
    "requirejs-text": "^2.0.12",
    "sass-loader": "^3.2.0",
    "slick-carousel": "^1.6.0",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "superagent": "^3.3.0",
    "svg-url-loader": "^1.1.0",
    "url-loader": "^0.5.7",
    "util": "^0.10.3",
    "webpack": "^1.13.0",
    "xml2js": "^0.4.19",
    "zipcodes": "^8.0.0"
  },
  "devDependencies": {}
}
