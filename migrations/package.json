{
  "name": "vision-wheel-migrations",
  "version": "0.0.0",
  "description": "Migration for VW Postgres Database",
  "main": "index.js",
  "author": "Mirum",
  "license": "ISC",
  "config": {
    "port": "8080",
    "path": "./:./../common/",
    "migrationsdir": "./vwd-migrations"
  },
  "engines": {
    "node": ">=6.2.1",
    "npm": "~2.15.1"
  },
  "scripts": {
    "env": "env",
    "inspector": "node-inspector",
    "server": "node $npm_package_main",
    "server-debug": "node --debug $npm_package_main",
    "server-debug-brk": "node --debug-brk $npm_package_main",
    "start": "NODE_PATH=$npm_package_config_path npm run server",
    "start:debug": "NODE_PATH=$npm_package_config_path npm run server-debug",
    "start:debug-brk": "NODE_PATH=$npm_package_config_path npm run server-debug-brk",
    "start:test": "NODE_ENV=test Q_DEBUG=1 npm start",
    "start:test:debug": "NODE_ENV=test Q_DEBUG=1 npm run start:debug",
    "start:test:debug-brk": "NODE_ENV=test Q_DEBUG=1 npm run start:debug-brk",
    "start:dev": "NODE_ENV=development Q_DEBUG=1 npm start",
    "start:dev:debug": "NODE_ENV=development Q_DEBUG=1 npm run start-debug",
    "start:dev:debug-brk": "NODE_ENV=development Q_DEBUG=1 npm run start-debug-brk",
    "start:prod": "NODE_ENV=production Q_DEBUG= DEBUG=vwd:* npm start -- --no-color",
    "start:prod:debug": "NODE_ENV=production Q_DEBUG= DEBUG=vwd:*, npm run start-debug -- --no-color",
    "start:prod:debug-brk": "NODE_ENV=production Q_DEBUG= DEBUG=vwd:*, npm run start-debug-brk -- --no-color",
    "clean": "sh ./bin/clean-nm.sh",
    "postinstall": "npm i pg db-migrate pg-migrate db-migrate-pg node-pg-migrate node-db-migrate -g",
    "create:test:migration": "node_modules/db-migrate/bin/db-migrate -m $npm_package_config_migrationsdir create:test createTestMigration",
    "create:test:db": "node_modules/db-migrate/bin/db-migrate -m $npm_package_config_migrationsdir db:create visionwheeldealers_test",
    "drop:test:db": "node_modules/db-migrate/bin/db-migrate -m $npm_package_config_migrationsdir db:drop visionwheeldealers_test",
    "down:test:migration": "node_modules/db-migrate/bin/db-migrate -m $npm_package_config_migrationsdir down:test",
    "reset:test:migration": "node_modules/db-migrate/bin/db-migrate -m $npm_package_config_migrationsdir reset:test",
    "up:test:migration": "node_modules/db-migrate/bin/db-migrate -m $npm_package_config_migrationsdir up:test upTestMigration -v"
  },
  "repository": {
    "type": "git",
    "url": "git@int-gitlab.aws.lbox.com:Mirum/vision-wheel-dealers.git"
  },
  "keywords": [
    "vision",
    "wheel",
    "dealers"
  ],
  "dependencies": {
    "async": "^2.1.4",
    "chalk": "^1.1.3",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "commander": "^2.9.0",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "db-migrate-sqlite3": "^0.2.1",
    "dotenv": "^4.0.0",
    "figlet": "^1.2.0",
    "fullname": "^3.3.0",
    "fullname-native": "^2.0.0",
    "helprs": "^0.21.1",
    "inquirer": "^3.0.1",
    "memwatch-next": "^0.3.0",
    "moment": "^2.17.1",
    "node-db-migrate": "^1.0.1",
    "node-pg-migrate": "0.0.13",
    "npm": "^3.8.9",
    "pg": "^6.1.2",
    "pg-migrate": "^2.0.0",
    "pg-promise": "^5.5.6",
    "pkginfo": "^0.4.0",
    "preferences": "^0.2.1",
    "q": "^1.4.1",
    "rc": "^1.1.6",
    "requirejs": "^2.2.0",
    "requirejs-text": "^2.0.12",
    "underscore": "^1.8.3",
    "username": "^2.3.0",
    "winston": "^2.3.0",
    "winston-daily-rotate-file": "^1.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.0.4",
    "chai": "^3.5.0",
    "eslint": "^3.14.1",
    "eslint-release": "^0.10.1",
    "express-debug": "^1.1.1",
    "husky": "^0.13.1",
    "istanbul": "^0.4.3",
    "jscs": "^3.0.7",
    "jshint": "^2.9.4",
    "jsonfile": "^2.4.0",
    "mocha": "^2.5.3",
    "node-debug": "^0.1.0",
    "node-inspector": "^0.12.8",
    "rimraf": "^2.5.4",
    "stack-trace": "0.0.9",
    "supertest": "^1.2.0"
  }
}
