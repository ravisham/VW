{
  "name": "vision-wheel-dealers",
  "version": "0.1.0",
  "description": "Transform the existing Vision Wheel site and make it eCommerce enabled for the companyâ€™s retail dealers and OEM/OES customers",
  "main": "server/server.js",
  "author": "Mirum",
  "license": "ISC",
  "config": {
    "port": "8080",
    "path": "./:./../common/"
  },
  "scripts": {
    "env": "env",
    "inspector": "node-inspector",
    "items": "NODE_ENV=development NODE_PATH=$npm_package_config_path node ./repositories/product_item_population.js",
    "update-item-images-protocal": "NODE_ENV=development NODE_PATH=$npm_package_config_path node ./repositories/updateImageProtocal.js",
    "update-product-images-protocal": "NODE_ENV=development NODE_PATH=$npm_package_config_path node ./repositories/updateProductImageProtocal.js",
    "update-item-images": "NODE_ENV=development NODE_PATH=$npm_package_config_path node ./repositories/ImageMapping.js",
    "update-item-images-rename": "NODE_ENV=development NODE_PATH=$npm_package_config_path node ./repositories/ImageRename.js",
    "update-product-images": "NODE_ENV=development NODE_PATH=$npm_package_config_path node ./repositories/ProductImageMapping.js",
    "autoprefixer": "postcss -u --local-plugins -u autoprefixer -m file -r ./release/css/visionwheel.css",
    "build": "webpack",
    "build:watch": "webpack --colors --progress --watch",
    "postsass": "npm run autoprefixer",
    "server": "node ./server/server.js",
    "server-debug": "node --debug ./server/server.js",
    "server-debug-brk": "node --debug-brk ./server/server.js",
    "server:watch": "NODE_ENV=development NODE_PATH=$npm_package_config_path PORT=8001 nodemon --stack-trace-limit=1000 ./server/server.js --ignore ./release --ignore ./server/scripts",
    "server-debug:watch": "nodemon --debug ./server/server.js",
    "start": "NODE_PATH=$npm_package_config_path npm run server",
    "start-prod": "NODE_ENV=production npm start",
    "lint": "eslint -f table server/ > eslint-debug.log",
    "test": "npm run lint",
    "unit-test": "mocha tests/models.js",
    "start-qa-docker": "docker build -t $npm_package_name . && docker run -it -e NODE_ENV=qa -p 8080:8080 $npm_package_name"
  },
  "repository": {
    "type": "git",
    "url": "git@int-gitlab.aws.lbox.com:Mirum/vision-wheel-dealers.git"
  },
  "keywords": [
    "vision",
    "wheel",
    "dealers"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "aws-sdk": "^2.54.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.0",
    "camelcase": "^3.0.0",
    "commander": "^2.9.0",
    "compression": "^1.6.1",
    "css-loader": "^0.23.1",
    "csv-parse": "^1.2.4",
    "csv-stringify": "^2.0.0",
    "es5-shim": "^4.5.8",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-release": "^0.5.0",
    "express": "^4.13.4",
    "express-session": "^1.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.3",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.1.2",
    "json-loader": "^0.5.4",
    "jwt-simple": "^0.5.0",
    "list.js": "^1.5.0",
    "lodash": "^4.11.2",
    "method-override": "^2.3.5",
    "moment": "^2.17.1",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "node-sass": "^3.7.0",
    "npm": "^3.8.9",
    "npm-license": "^0.3.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-beta6",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "redbox-react": "^1.3.1",
    "requirejs": "^2.2.0",
    "requirejs-text": "^2.0.12",
    "sass-loader": "^3.2.0",
    "slick-carousel": "^1.6.0",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "superagent": "^3.3.1",
    "svg-url-loader": "^1.1.0",
    "url-loader": "^0.5.7",
    "util": "^0.10.3",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "jsonfile": "^2.4.0",
    "node-debug": "^0.1.0",
    "node-inspector": "^0.12.8",
    "nodemon": "^1.11.0",
    "postcss-cli": "^2.5.2",
    "rewire": "^2.5.1",
    "stack-trace": "0.0.9",
    "supertest": "^1.2.0"
  }
}
